    "@contextlib.contextmanager\n",
    "def file_open(file_name):\n",
    "    # 类似 __enter__\n",
    "    print('file open')\n",
    "    # 生成器\n",
    "    yield {}\n",
    "    # 类似 __exit___\n",
    "    print('file end')"
